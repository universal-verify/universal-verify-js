  "bugs": {
    "url": "https://github.com/universal-verify/universal-verify-js/issues"
  },
  "homepage": "https://github.com/universal-verify/universal-verify-js#readme",
  "dependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.32.1"
  },
  "devDependencies": {
    "http-server": "^14.1.1"
  }
}
