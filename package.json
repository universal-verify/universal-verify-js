{
  "name": "universal-verify-js",
  "version": "0.0.1",
  "description": "SDK for integrating with Universal Verify",
  "main": "build/universal-verify.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
